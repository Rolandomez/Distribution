enablePlugins(ScalaJSPlugin)
libraryDependencies ++= Seq(
  "com.olvind" %%% "scalablytyped-runtime" % "2.1.0",
  "org.scalablytyped" %%% "multiaddr" % "8.1.1-52bd3a",
  "org.scalablytyped" %%% "std" % "4.1-c651d0")
