  "com.olvind" %%% "scalablytyped-runtime" % "2.1.0",
  "org.scalablytyped" %%% "std" % "3.9-bafc13",
  "org.scalablytyped" %%% "xml-core" % "1.0.24-3cd456",
  "org.scalablytyped" %%% "xmldsigjs" % "2.0.31-38c084")
